{
  "manifest_version": 3,

  "name": "Flipd Bumper",
  "description": "Bumps posts to make them more accessible.",
  "version": "3.0",

  "icons": { "16": "icon.png"},
  "permissions": ["unlimitedStorage"],
  "content_scripts": [
    {
      "matches": ["https://flipd.gg/*"],
      "js": ["inject.js"]
    }
  ],
  "web_accessible_resources": [
	{
      "resources": ["aes.js", "main.js"],
      "matches": [ "https://flipd.gg/*" ]
    }
  ]
}
